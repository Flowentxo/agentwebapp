{
  "name": "sintra-system",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cross-env NODE_OPTIONS=--max-old-space-size=2048 nodemon --exec \"tsx --env-file=.env.local server/index.ts\"",
    "build": "next build",
    "build:backend": "tsc --project tsconfig.server.json",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "next start",
    "start:backend": "tsx --env-file=.env.local server/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx lib/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-knowledge.ts",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest --watch",
    "test:unit:ui": "vitest --ui",
    "test:api": "playwright test tests/api",
    "test:smoke": "playwright test tests/api/smoke.spec.ts",
    "test:rate": "playwright test tests/api/rate-limit.spec.ts",
    "test:redact": "playwright test tests/api/secrets-redaction.spec.ts",
    "test:redact:cloud": "playwright test tests/api/cloud-secrets-redaction.spec.ts",
    "test:logs": "playwright test tests/api/logs-redaction.spec.ts",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:ui": "playwright test tests/e2e --ui",
    "test:e2e:headed": "playwright test tests/e2e --headed",
    "test:e2e:chromium": "playwright test tests/e2e --project=chromium",
    "test:e2e:firefox": "playwright test tests/e2e --project=firefox",
    "test:e2e:webkit": "playwright test tests/e2e --project=webkit",
    "test:e2e:inbox": "playwright test tests/e2e/inbox.spec.ts",
    "test:e2e:report": "playwright show-report",
    "test:predictive": "tsx scripts/test-predictive-context.ts",
    "test:calendar": "tsx scripts/test-predictive-context.ts",
    "lint": "next lint",
    "redis:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-redis.ps1",
    "redis:check": "npx ts-node scripts/check-redis.ts",
    "clean:repo": "node scripts/cleanup-repo.js --dry-run",
    "clean:repo:apply": "node scripts/cleanup-repo.js",
    "strict:clean": "node scripts/strict-clean.mjs",
    "strict:clean:apply": "node scripts/strict-clean.mjs --apply",
    "worker": "tsx --env-file=.env.local workers/agent-execution-worker.ts",
    "worker:dev": "nodemon --exec \"tsx --env-file=.env.local workers/agent-execution-worker.ts\"",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run worker:dev\"",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "build:static": "node scripts/build-tauri.mjs",
    "dev:tauri": "concurrently \"npm run dev:frontend\" \"npm run tauri:dev\""
  },
